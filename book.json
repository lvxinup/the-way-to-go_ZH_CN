{
"author" : "Ivo Balbaert",
"description" : "《The Way to Go》中文译本",
"language" : "zh-hans",
"title" : "Go入门指南",
"plugins": [
    "splitter",
    "todo",
    "alerts",
    "anchors",
    "-lunr", "-search", "search-plus",
    "advanced-emoji",
    "-sharing", "sharing-plus",
    "simple-page-toc",
    "anchor-navigation-ex",
    "theme-comscore",
    "download-pdf-link",
    "code",
    "-highlight", "prism", "prism-themes",
    "favicon-plus"
],
"pluginsConfig": {
    "sharing": {
       "douban": false,
       "facebook": false,
       "google": true,
       "hatenaBookmark": false,
       "instapaper": false,
       "line": true,
       "linkedin": true,
       "messenger": false,
       "pocket": false,
       "qq": false,
       "qzone": true,
       "stumbleupon": false,
       "twitter": false,
       "viber": false,
       "vk": false,
       "weibo": true,
       "whatsapp": false,
       "all": [
           "facebook", "google", "twitter",
           "weibo", "instapaper", "linkedin",
           "pocket", "stumbleupon"
       ]
   },
   "simple-page-toc": {
       "maxDepth": 3,
       "skipFirstH1": true
   },
   "anchor-navigation-ex": {
       "showLevel": true,
       "associatedWithSummary": true,
       "printLog": false,
       "multipleH1": false,
       "mode": "float",
       "float": {
           "showLevelIcon": true,
           "level1Icon": "fa fa-hand-o-right",
           "level2Icon": "fa fa-hand-o-right",
           "level3Icon": "fa fa-hand-o-right"
       },
       "pageTop": {
           "showLevelIcon": false,
           "level1Icon": "fa fa-hand-o-right",
           "level2Icon": "fa fa-hand-o-right",
           "level3Icon": "fa fa-hand-o-right"
       }
   },
   "download-pdf-link": {
     "base": "http://default-1252251317.file.myqcloud.com/the-way-to-go_ZH_CN.pdf",
      "label": "Download PDF",
      "multilingual": false
   },
   "prism": {
     "css": [
       "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
     ]
   },
   "favicon-plus": {
     "favicon": "./favicon-96x96.png"
   }
}
}
